{
  "name": "webforms-ui",
  "version": "6.7.0",
  "private": false,
  "scripts": {
    "start": "export VITE_APP_VERSION=$(git describe --tags) && vite",
    "build": "export VITE_APP_VERSION=$(git describe --tags) && vite build",
    "build:ov": "npm run build -- --mode=ov",
    "build:prod": "npm run build -- --mode=prod",
    "build:pre": "npm run build -- --mode=pre",
    "build:vercel": "npm run build -- --mode=vercel",
    "test": "TZ=Europe/Madrid vitest",
    "test:coverage": "TZ=Europe/Madrid vitest run --coverage",
    "cypress": "TZ=Europe/Madrid cypress open",
    "cypress:smoke": "TZ=Europe/Madrid cypress run --env configFile=production",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "i18n-extract": "./node_modules/.bin/i18next src/**/*.js* -o src/i18n/locale-xx.json -s",
    "i18n-lint": "./scripts/lint-i18n.sh",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-facebook-pixel": "^1.0.4",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.2.2",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/x-date-pickers": "^6.5.0",
    "@somenergia/somenergia-ui": "^1.0.0-alpha.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@googlemaps/js-api-loader": "^1.16.6",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "formik": "^2.2.9",
    "i18next": "^23.11.2",
    "ibantools": "^4.5.1",
    "libphonenumber-js": "^1.12.6",
    "plausible-tracker": "^0.3.8",
    "stdnum": "^1.11.0",
    "yup": "^0.32.11"
  },
  "prettier": "prettier-config-standard",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "trustedDependencies": [
    "@somenergia-ui/somenergia-ui",
    "@swc/core",
    "cypress",
    "esbuild"
  ],
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "cypress": "^12.17.4",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.0.0",
    "prettier-config-standard": "^5.0.0",
    "i18next-parser": "^9.0.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.6.0"
  }
}
