{
  "name": "webforms-ui",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "axios": "^0.19.2",
    "formik": "^2.1.4",
    "i18next": "^19.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "yup": "^0.28.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && yarn run build-rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "yarn run build-rename-js && yarn run build-rename-css",
    "build-rename-js": "renamer --find '/((runtime-)?main)\\.\\w*(\\.chunk)?(.js(.map)?)/i' --replace '$1$4' build/static/js/*.* && renamer --find '/\\w\\.\\w*(\\.chunk)?(.js(.map)?)/i' --replace 'vendors$2' build/static/js/*.*",
    "build-rename-css": "renamer --find '/(main)\\.\\w*(\\.chunk)?(.css(.map)?)/i' --replace '$1$3' build/static/css/*.* && renamer --find '/\\w\\.\\w*(\\.chunk)?(.css(.map)?)/i' --replace 'vendor$2' build/static/css/*.*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "renamer": "^2.0.0",
    "standard": "^14.3.3"
  }
}
