{
  "name": "webforms-ui",
  "version": "1.9.6",
  "private": false,
  "dependencies": {
    "@date-io/dayjs": "1.x",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.8",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "i18next": "^21.6.13",
    "plausible-tracker": "^0.3.8",
    "preval.macro": "^5.0.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.15.5",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "export REACT_APP_VERSION=$(git rev-parse HEAD) && react-scripts start",
    "build": "export REACT_APP_VERSION=$(git rev-parse HEAD) && react-scripts build",
    "build:ov": "env-cmd -f .env.ov  --use-shell \"npm run build && ./scripts/rename-assets-ov.sh\"",
    "build:staging": "env-cmd -f .env.staging --use-shell \"npm run build\"",
    "build:wp": "env-cmd -f .env.wp --use-shell \"npm run build\"",
    "build:vercel": "env-cmd -f .env.vercel --use-shell \"npm run build\"",
    "test": "react-scripts test",
    "cypress": "./node_modules/.bin/cypress open",
    "eject": "react-scripts eject"
  },
  "prettier": "prettier-config-standard",
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "cypress-file-upload": "^5.0.8",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier-config-standard": "^5.0.0",
    "standard": "^16.0.4"
  }
}
