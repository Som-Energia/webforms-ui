{
  "name": "webforms-ui",
  "version": "1.2.0",
  "private": false,
  "homepage": "/wp-content/plugins/react-wordpress/webforms/build",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "axios": "^0.21.0",
    "clsx": "^1.1.1",
    "formik": "^2.1.4",
    "i18next": "^19.8.3",
    "preval.macro": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.2.1",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.7.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "yup": "^0.28.5"
  },
  "scripts": {
    "start": "export REACT_APP_VERSION=$(git rev-parse HEAD) && react-scripts start",
    "build": "export REACT_APP_VERSION=$(git rev-parse HEAD) && react-scripts build",
    "build:rename": "yarn run build && yarn run build-rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "yarn run build-rename-js && yarn run build-rename-css",
    "build-rename-js": "renamer --find '/((runtime-)?main)\\.\\w*(\\.chunk)?(.js(.map)?)/i' --replace '$1$4' build/static/js/*.*",
    "build-rename-css": "renamer --find '/(main)\\.\\w*(\\.chunk)?(.css(.map)?)/i' --replace '$1$3' build/static/css/*.*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^6.4.0",
    "cypress-file-upload": "^5.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "renamer": "^2.0.0",
    "standard": "^14.3.3"
  }
}
